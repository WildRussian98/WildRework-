"VertexLitGeneric"
{
	$basetexture "models/OCDEV/cars/generic/glass2"
	$bumpmap "models/OCDEV/cars/generic/nobump_dirtyglass"
	$surfaceprop "glass"
	$translucent "1"
	$nodiffusebumplighting "1"
//	$nocull "1"
//	$additive "1"

	$envmap "env_cubemap"
	$normalmapalphaenvmapmask "1"
	$envmaptint "[.9 1.1 1]"
//	$envmapcontrast "1"
//	$envmapsaturation ".5"
	$envmapfresnel "1"
//	$envmapfresnelminmaxexp "[.01 .5 1]"

	$phong "1"
	$phongexponent "5"
	$phongboost ".15"
	$phongfresnelranges "[.5 1 2]"
	$invertphongmask "1"
	"$colorfix" "{255 255 255}"
	"Proxies"
	{
		"Equals"
		{
			srcVar1 $colorfix
			resultVar	$color
		}
	}
}
